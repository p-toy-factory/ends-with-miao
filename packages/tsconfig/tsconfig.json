{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"allowUmdGlobalAccess": true,
		"declaration": true,
		"esModuleInterop": true,
		"module": "ESNext",
		"moduleResolution": "node",
		"noFallthroughCasesInSwitch": true,
		"noImplicitOverride": true,
		"resolveJsonModule": true,
		"strict": true,
		"target": "ESNext"
	}
}

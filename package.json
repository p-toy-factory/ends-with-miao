{
  "private": "true",
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "pnpm@8.6.2",
  "scripts": {
    "build:lib": "pnpm run -C packages/ends-with-miao build",
    "build:userscript": "pnpm run build:lib && pnpm run -C packages/userscript build",
    "lint": "pnpm run -C packages/userscript lint",
    "lint:fix": "pnpm run -C packages/userscript lint:fix",
    "test": "pnpm run -C packages/ends-with-miao test"
  }
}
